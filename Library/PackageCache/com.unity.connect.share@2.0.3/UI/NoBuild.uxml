<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:VisualElement name="MainWindow" class="main-container main-container-light">
        <Style src="Styles.uss" />
        <ui:VisualElement name="imgLogo" class="status-icon status-icon-webgl-module" />
        <ui:Label text="Create a WebGL Build" name="lblTitle" class="progress-label" />
        <ui:Label text="Note that the first build might take a while and that the editor will be unresponsive. The next build will be faster." name="lblInstructions" class="info-text" />
    </ui:VisualElement>
    <ui:Button text="Create New WebGL Build" name="btnBuild" class="bottom-button bottom-button-blue">
        <Style src="Styles.uss" />
    </ui:Button>
    <ui:Button text="Locate Existing Build" name="btnLocateExisting" class="bottom-button bottom-button-locate">
        <Style src="Styles.uss" />
    </ui:Button>
</ui:UXML>
